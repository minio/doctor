<footer id="footer">
	<% if @mybrand %>
        <%=  @mybrand.footer_text %>

        <% if !@mybrand.footer_img.blank? %>
            <img class="footer__img" src="<%= @mybrand.footer_img %>" alt="">
        <% end %>
			 
	<% else %>
        Doctor is licensed under Apache 2.0 License.
	<% end %>

    <%= (link_to "Talk to community", @mybrand.footer_chatlink, :target => "_blank", :class => "footer__chatlink") if !@mybrand.footer_chatlink.blank? %>
</footer>